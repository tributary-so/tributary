{
  "name": "@tributary/sdk",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -rf ./lib",
    "build": "pnpm run clean && npm run build:esm",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv lib/esm/sdk/src/index.js lib/esm/sdk/src/index.mjs && echo '{\"type\": \"module\"}' > lib/esm/package.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json",
    "manager": "npx tsx src/manager.ts",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.0",
    "@solana/web3.js": "^1.87.0",
    "bn.js": "^4.12.1",
    "commander": "^14.0.1"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/**/*",
    "src/**/*.json"
  ],
  "exports": {
    ".": {
      "require-not-supported": "./lib/cjs/sdk/src/index.js",
      "import": "./lib/esm/sdk/src/index.mjs",
      "types": "./lib/types/sdk/src/index.d.ts"
    }
  },
  "main": "./lib/sdk/src/index.js",
  "module": "./lib/sdk/src/index.js",
  "types": "./lib/types/sdk/src/index.d.ts",
  "packageManager": "pnpm@9.6.0"
}
